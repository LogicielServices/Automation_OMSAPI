<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="IDENTITY SERVER - ADMIN USER">
	
			 <test thread-count="5" name="Admin User Login  API">
			    <classes>
					<class name="IdentityServer_API_Admin.AdminLogin">
						     <methods>
					         	 <include name="AdminLoginAuthentications"/>
					         </methods>
					</class>
				</classes>
			 </test> <!-- Test: Admin User Login-->
			  
		
			 <test thread-count="5" name="User Registeration API">
			    <classes>
					<class name="IdentityServer_API_Admin.UserRegister">
							 <methods>
					         	 <include name="Verify_User_Registeration"/>
					         </methods>
					</class>
				</classes>
			 </test> <!-- Test: User Registeration-->
			  
		
		  
			 <test thread-count="5" name="User Account Register API">
			    <classes>
					<class name="IdentityServer_API_Admin.UserAccountsRegister">
						     <methods>
					         	 <include name="Verify_User_Account_Register"/>
					         </methods>
					</class>
				</classes>
			 </test> <!-- Test : User Account Register -->
		  
  
	 	     <test thread-count="5" name="User Account Associate  API">
			    <classes>
					<class name="IdentityServer_API_Admin.UserAccountsAssociate">
						     <methods>
					         	 <include name="Verify_User_Account_Associate"/>
					         </methods>
					</class>
				</classes>
			  </test> <!-- Test : User Account Associate -->
			  
					  
			  <test thread-count="5" name="Update User Status  API">
			    <classes>
					<class name="IdentityServer_API_Admin.UpdateUserStatus">
						     <methods>
					         	 <include name="Verify_Update_User_Status"/>
					         </methods>
					</class>
				</classes>
			  </test> <!-- Test : Update User Status -->
			  
			  <test thread-count="5" name="Update User Info  API">
			    <classes>
					<class name="IdentityServer_API_Admin.UpdateUserInfo">
						     <methods>
					         	 <include name="Verify_Update_User_Info"/>
					         </methods>
					</class>
				</classes>
			  </test> <!-- Test : Update User Info -->
				  
  
  
</suite> <!-- Suite -->
