<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EQUITY ACTIVE-OPEN ORDERS">
		  
		<test thread-count="1" name="EquityOrder Active-Open Creation">
		    <classes>
			 <class name="IdentityServer_API_User.Login"/>
			 <class name="Orders.Create">
			    <methods>
			         <include name="Verify_Equity_Order_Active_Creation"/>
			    </methods>
      	      </class>
      	      <class name="Subscriptions.Equity_Order_Subscribe">
			    <methods>
			         <include name="Verify_SubscribeActiveEquityOrder"/>
			    </methods>       
	      	  </class>
	      	</classes>
		</test> 
		  
		  
		  
	
		<test thread-count="1" name="Update-Delete-Cancel EquityOrder">
		    <classes>
			  <class name="IdentityServer_API_User.Login"/>
			  <class name="Orders.Update"/>
			  <class name="Orders.Cancel"/>
			  <class name="Orders.Delete"/>
			  			 
	      	</classes>
		</test> 
		
		
		
		
</suite> <!-- Suite -->		  

