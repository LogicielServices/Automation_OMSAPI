<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OPTION ORDERS CREATION">
		  
		  
		 <test thread-count="5" name="User Login">
		    <classes>
				<class name="IdentityServer_API_User.Login">
				    <methods>
				         <include name="UserLogin"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: User Login -->
	
	
		<test thread-count="5" name="OptionOrder Active-Open Creation">
		    <classes>
				<class name="OptionOrders.Create_OptionOrder">
				    <methods>
				         <include name="Verify_Order_Creation_Active_Open_Status"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Option_Order_Active_Open_Creation -->
		
		  
		<test thread-count="5" name="OptionOrder Rejected Creation">
		    <classes>
				<class name="OptionOrders.Create_OptionOrder">
				    <methods>
				         <include name="Verify_Order_Creation_Rejected_Status"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Option_Order_Rejected_Creation -->
		  
		  		  
		 <test thread-count="5" name="OptionOrder Buy Filled Creation">
		    <classes>
				<class name="OptionOrders.Filled_OptionOrder">
				    <methods>
				         <include name="Verify_BUY_Order_Creation_Filled"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Verify_BUY_Option_Order_Filled_Creation -->
		
		  
		 <test thread-count="5" name="OptionOrder Sell Filled Creation">
		    <classes>
				<class name="OptionOrders.Filled_OptionOrder">
				    <methods>
				         <include name="Verify_SELL_Order_Creation_Filled"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Verify_SELL_Option_Order_Filled_Creation -->
		
		<test thread-count="5" name="Update OptionOrder">
		    <classes>
				<class name="OptionOrders.Update_OptionOrder">
				    <methods>
				         <include name="Verify_Update_Option_Order"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Verify_Update_Option_Order -->
		
		<test thread-count="5" name="Cancel OptionOrder">
		    <classes>
				<class name="OptionOrders.Cancel_OptionOrder">
				    <methods>
				         <include name="Verify_Cancel_Equity_Order"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Verify_Cancel_Option_Order -->
		
		<test thread-count="5" name="Delete OptionOrder">
		    <classes>
				<class name="OptionOrders.Delete_OptionOrder">
				    <methods>
				         <include name="Verify_Delete_Option_Order"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: Verify_Delete_Option_Order -->
			  
</suite> <!-- Suite -->
