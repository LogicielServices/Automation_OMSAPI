<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OPTION ORDERS SUBSCRIBE">
		  
		  
		<test thread-count="5" name="User Login">
		    <classes>
				<class name="IdentityServer_API_User.Login">
				    <methods>
				         <include name="UserLogin"/>
				    </methods>
	      	  </class>
	      	</classes>
		</test> <!-- Test: User Login -->
		
		<test thread-count="5" name="Subscribe Active OptionOrder">
		    <classes>
				<class name="Subscriptions.Option_Order_Subscribe">
				    <methods>
				         <include name="Verify_SubscribeActiveOptionOrder"/>
				    </methods>       
	      	  </class>
	      	</classes>
		</test> <!-- Test: Subscribe Active OptionOrder -->
		
		<test thread-count="5" name="Subscribe Buy Filled OptionOrder">
		    <classes>
				<class name="Subscriptions.Option_Order_Subscribe">
				    <methods>
				         <include name="Verify_Subscribe_BUYFilledOptionOrder"/>
				    </methods>       
	      	  </class>
	      	</classes>
		</test> <!-- Test: Subscribe BUY Filled OptionOrder -->
		
		<test thread-count="5" name="Subscribe Sell Filled OptionOrder">
		    <classes>
				<class name="Subscriptions.Option_Order_Subscribe">
				    <methods>
				         <include name="Verify_Subscribe_SELLFilledOptionOrder"/>
				    </methods>       
	      	  </class>
	      	</classes>
		</test> <!-- Test: Subscribe SELL Filled OptionOrder -->
				
		<test thread-count="5" name="Subscribe Rejected OptionOrder">
		    <classes>
				<class name="Subscriptions.Option_Order_Subscribe">
				    <methods>
						<include name="Verify_SubscribeRejectedOptionOrder"/>
				    </methods>       
	      	  </class>
	      	</classes>
		</test> <!-- Test: Subscribe Rejected OptionOrder -->
		
		
		
		
		 

		 	  
</suite> <!-- Suite -->
